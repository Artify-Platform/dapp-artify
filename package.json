{
  "name": "artify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env/.env.development next dev --turbo",
    "start:dev": "env-cmd -f .env/.env.development next dev",
    "build:dev": "env-cmd -f .env/.env.development next build",
    "start:stag": "env-cmd -f .env/.env.staging next start",
    "build:stag": "env-cmd -f .env/.env.staging next build",
    "start:main": "env-cmd -f .env/.env.production next start",
    "build:main": "env-cmd -f .env/.env.production next build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "postinstall": "husky",
    "pre-commit": "lint-staged",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@mysten/sui.js": "^0.51.2",
    "@mysten/zklogin": "^0.5.2",
    "@polymedia/suits": "^0.0.41",
    "@solana/spl-token": "^0.4.1",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.31",
    "@solana/web3.js": "^1.91.1",
    "@suiet/wallet-kit": "^0.2.24",
    "@types/axios": "^0.14.0",
    "@types/react-speech-recognition": "^3.9.5",
    "@types/regenerator-runtime": "^0.13.5",
    "@web3modal/wagmi": "3.5.7",
    "axios": "^1.7.2",
    "framer-motion": "^11.1.7",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "next": "14.2.1",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-infinite-scroll-component": "^6.1.0",
    "react-speech-recognition": "^3.10.0",
    "react-toastify": "^10.0.5",
    "react-use-downloader": "^1.2.8",
    "regenerator-runtime": "^0.14.1",
    "viem": "1.21.4",
    "wagmi": "1.4.13",
    "wallet-kit-monorepo": "suiet/wallet-kit"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
